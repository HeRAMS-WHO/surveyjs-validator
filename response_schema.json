{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://github.com/HeRAMS-WHO/surveyjs-validator/response_schema.json",
  "type": "object",
  "title": "The root schema",
  "description": "Response schema for SurveyJS-validator",
  "default": {},
  "examples": [
    {
      "valid": false
    }
  ],
  "required": [
    "valid"
  ],
  "properties": {
    "valid": {
      "$id": "#/properties/valid",
      "type": "boolean",
      "title": "Whether the data is valid for the given structure",
      "default": false,
      "examples": [
        false,
        true
      ]
    }
  },
  "additionalProperties": true
}
